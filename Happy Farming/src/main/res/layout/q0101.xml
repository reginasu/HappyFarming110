<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/q0101_lay01"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Q0101">

    <LinearLayout
        android:id="@+id/q0101_lay03"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/hf_back"
        android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center|center_horizontal|center_vertical"
                app:srcCompat="@drawable/hf_logo_1000" />

    </LinearLayout>

    <TextView
        android:id="@+id/q0101_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/p120"
        android:gravity="center"
        android:text="@string/q0101_t001"
        android:textColor="@color/seagreen"
        android:textSize="34sp"
        android:textStyle="bold" />
</RelativeLayout>